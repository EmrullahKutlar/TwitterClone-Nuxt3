{
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {
            "serverFiles": ["server/**"]
        }
      }
    ],
    "routes": [
        {
          "src": "/server/middleware/(.*)",
          "middlewarePath": "auth",
          "continue": true
        }
      ]
}